{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "code": { "type": "integer", "enum": [200] },
    "message": { "type": "string", "enum": ["success"] },
    "data": {
      "type": "object",
      "properties": {
        "id": { "type": "integer" },
        "username": { "type": "string" },
        "token": { "type": "string" }
      },
      "required": ["id", "username", "token"]
    }
  },
  "required": ["code", "message", "data"]
}
